
{% extends "layout.html.njk" %}

{% block pagetitle %}{{ obj.cater.page_title }}{% endblock %}
{% block pagedesc %}{{ obj.cater.page_description }}{% endblock %}
{% block main_class %}{{ obj.cater.id }}{% endblock %}
{% block navie_active %}{{ obj.cater.navie_active }}{% endblock %}
{% block logo %} <div id="logo" src="{{ obj.cater.logo_url }}" target="/index.html"></div> {% endblock %}

{% block content %}

	   <!-- Banner start -->
	   <section id="banner" class="banner">
			<div class="banner-overlay"></div>
			<div class="banner-title">
				<h1>Traiteur</h1>
			</div>
	   </section>
	   <!-- Banner end -->

	      <!-- Drawings start -->
	      <section id="drawings" class="drawings">
	      	<div class="drawings-item ">
	      		<div class="drawing-picture flce" style="background-image:url(img/illu_petitdej.jpg)"></div>
	   		<p>Petit déjeuner</p>
	      	</div>
	      	<div class="drawings-item">
	      		<div class="drawing-picture flce" style="background-image:url(img/illu_lunch.jpg)"></div>
	   		<p>Lunch</p>
	      	</div>
	      	<div class="drawings-item">
	      		<div class="drawing-picture flce" style="background-image:url(img/illu_apero.jpg)"></div>
	   		<p>Apéritif dinatoire</p>
	      	</div>
      	   	<div class="drawings-item">
      	   		<div class="drawing-picture flce" style="background-image:url(img/illu_barbecue.png)"></div>
      			<p>Barbecue</p>
      	   	</div>
	      </section>
	      <!-- Drawings end -->



	   	<!-- Instant details (Smoothies) start -->
	   	<div class="instant-section x--breakfast">
	   		<div class="instant-header">
	   			<div class="instant-splitter x--short"></div>
	   			<div class="instant-title"><h1>Petit déjeuner</h1></div>
	   			<div class="instant-splitter"></div>
	   		</div>
	   		<div class="instant-body imt">
	   			<div class="instant-body-img imt-picture"></div>
	   			<div class="instant-body-content imt-content">
	   				<div class="instant-details">
	   					<div class="instant-details-title">
	   						<h1>Le concept</h1>
	   					</div>
	   					<p>Nous vous proposons une offre complète à composer selon vos envies et adaptée à tous vos évènements pour bien débuter votre journée.</p>
	   				</div>
	   				<div class="instant-details-quote">
	   					<button>Demander un devis <a href="/devis.html"></a></button>
	   				</div>
	   			</div>
	   		</div>
	   	</div>
	   	<!-- Instant details (Smoothies) end -->


	   	<!-- Instant details (Salads) start -->
