#mason div:hover .masonlay {
  opacity: 0.5;
}

#mason div {
  overflow: hidden;
  border-radius: 3px;
}

#mason .masontxt {
  font-family: 'caviar-bold';
  font-size: 13px;
  position: absolute;
  top: 50%;
  left: 50%;
  -webkit-transform: translate(-50%, -50%);
  transform: translate(-50%, -50%);
  -webkit-transition: all ease-in-out .25s;
  transition: all ease-in-out .25s;
  z-index: 2;
  color: white;
  opacity: 0;
  text-align: center;
  width: 100%;
}

#mason div:hover img {
  -webkit-transform: scale(1.05);
  transform: scale(1.05);
}

#mason div:hover .masontxt {
  -webkit-transform: translate(-50%, -55%);
  transform: translate(-50%, -55%);
  opacity: 1;
}

#mason img {
  max-width: 100%;
  max-height: 100%;
  -webkit-transition: all ease-in-out .25s;
  transition: all ease-in-out .25s;
}

.masonlay {
  width: 100%;
  height: 100%;
  background: #000;
  opacity: 0;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 1;
  -webkit-transition: all ease-in-out .2s;
  transition: all ease-in-out .2s;
  cursor: pointer;
}

