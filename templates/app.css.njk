/* Navigation header */

.navie {
  z-index: 10;
  position: fixed;
}

.navie-sublist {
  border: none!important;
}

.navie-item {
  border: none!important;
}

.navie[active="landing"].x--sticky {
  position: fixed !important;
  top: -170px;
  transition: none;
}

.navie[active="landing"]:not(.x--responsive) {
  height: 240px;
  position: absolute;
  transition: none;
  border: none;
}

.navie[active="landing"]:not(.x--responsive) .navie-list {
  margin-top: auto;
  margin-bottom: 10px;
}

