ErrorDocument 404 /404.html

RewriteEngine On
RewriteBase /

Rewritecond %{HTTP_HOST} ^instant-b.fr$
Rewriterule ^(.*) http://www.instant-b.fr/$1 [QSA,L,R=301]

RewriteCond %{HTTPS} !=on
RewriteRule ^/?(.*) https://%{SERVER_NAME}/$1 [R,L]

RewriteCond %{REQUEST_URI} !^/v[0-9]
RewriteRule ^(.*)$ v2/$1 [L]

